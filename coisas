reverse
invertString :: String -> String
invertString palavra = reverse palavra
main :: IO()
main = do
    let palavra = "ola"
    print (invertString(palavra))



bixesto
bissexto :: Integer -> Bool
bissexto ano 
    | ano `mod` 400 == 0 = True
    | ano `mod` 100 == 0 = False
    | ano `mod` 4 == 0 = True
    |otherwise = False
main :: IO()
main = do
    let ano = 2030
    if bissexto ano
        then putStrLn ("É um ano bissexto!")
        else putStrLn ("Não é um ano bissexto!")
acerto alvo
score :: Float  -> Float -> Int
score x y 
    | dist <= 1 = 10
    | dist <= 5 = 5
    | dist <= 10 = 1
    | otherwise = 0
    where dist = sqrt (x*x+ y*y)
main :: IO()
main = do
    let x = 3
    let y = 4
    print(alvo(acerto))



  
  
